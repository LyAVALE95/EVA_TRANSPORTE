<p id="notice"><%= notice %></p>

<h1>Travel Stands</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @travel_stands.each do |travel_stand| %>
      <tr>
        <td><%= link_to 'Show', travel_stand %></td>
        <td><%= link_to 'Edit', edit_travel_stand_path(travel_stand) %></td>
        <td><%= link_to 'Destroy', travel_stand, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Travel Stand', new_travel_stand_path %>

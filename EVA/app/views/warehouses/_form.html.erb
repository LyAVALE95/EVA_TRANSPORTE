<%= form_with(model: warehouse, local: true) do |form| %>
  <% if warehouse.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(warehouse.errors.count, "error") %> prohibited this warehouse from being saved:</h2>

      <ul>
      <% warehouse.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
   <div class="col-md-12 block formulary">
        <div class="col-md-12 block">
         <div class="form-group">
            <%= form.label :name,"Nombre", class: "control-label  col-sm-1" %>
              <div class="col-sm-11 ">
                <%= form.text_field :name, id: :audit_results, class: "form-control border" %>
              </div>
          </div>
           <div class="form-group">
            <%= form.label :scheduleStart,"Horario entrada", class: "control-label  col-sm-1" %>
              <div class="col-sm-11 ">
                <%= form.text_field :scheduleStart, id: :audit_results, class: "form-control border" %>
              </div>
          </div>
           <div class="form-group">
            <%= form.label :scheduleEnd,"Horario salida", class: "control-label  col-sm-1" %>
              <div class="col-sm-11 ">
                <%= form.text_field :scheduleEnd, id: :audit_results, class: "form-control border" %>
              </div>
          </div>
            <div class="form-group">
            <%= form.label :scheduleDays,"Días laborales", class: "control-label  col-sm-1" %>
              <div class="col-sm-11 ">
                <%= form.text_field :scheduleDays, id: :audit_results, class: "form-control border" %>
              </div>
          </div>
            <div class="form-group">
            <%= form.label :tel,"Teléfono", class: "control-label  col-sm-1" %>
              <div class="col-sm-11 ">
                <%= form.text_field :tel, id: :audit_results, class: "form-control border" %>
              </div>
          </div>
            <div class="form-group">
            <%= form.label :location_id,"Localización", class: "control-label  col-sm-1" %>
              <div class="col-sm-11 form-control border">
                     <%= select(:warehouse, :location_id, @mylocations.map {|v| [ v['name'], v['id'] ] }) %>
              </div>
          </div>
            <div class="form-group">
            <%= form.label :clientcontact_id,"Contacto", class: "control-label  col-sm-1" %>
              <div class="col-sm-11 ">
                <%= form.text_field :clientcontact_id, id: :audit_results, class: "form-control border" %>
              </div>
          </div>
              <div class="form-group">
            <%= form.label :customer_id, "Cliente", class: "control-label col-sm-1" %>
              <div class="col-sm-11">
                <%= form.text_field :customer_id, id: :location_description, class: "form-control border" %>
              </div>
          </div>

        </div>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>

<p id="notice"><%= notice %></p>

<h1>Company Lines</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @company_lines.each do |company_line| %>
      <tr>
        <td><%= link_to 'Show', company_line %></td>
        <td><%= link_to 'Edit', edit_company_line_path(company_line) %></td>
        <td><%= link_to 'Destroy', company_line, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Company Line', new_company_line_path %>

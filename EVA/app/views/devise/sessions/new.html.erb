<div class="login">
<div  class="col-sm-6 col-md-6 col-lg-6 ">

<h1 class="login-heading">
      <strong>¡Bienvenido!</strong></h1>
<div class="form_log">
<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="field">
    <%= f.label :email, "Correo electrónico" %><br />
    <%= f.email_field :email, class: "input-txt" , autofocus: true %>
  </div>

  <div class="field">
    <%= f.label :password, "Contraseña" %><br />
    <%= f.password_field :password,class: "input-txt", autocomplete: "off" %>
  </div>

  <% if devise_mapping.rememberable? -%>
    <div class="field">
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me, "Guardar sesión" %>
    </div>
  <% end -%>
</div>
  <div class="actions">
    <%= f.submit "Ingresar", class: "login-submit" %>
  </div>
<% end %>



</div>
</div>